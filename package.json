  "description": "Elegant, responsive, flexible and lightweight notification plugin with no dependencies.=>ypc-toast",
  "readmeFilename": "README.md",
  "main": "dist/js/iziToast.js",
  "typings": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+git@github.com:youpengcheng/gk-notice.git"
  },
  "scripts": {
    "build": "webpack --mode development",
    "set_taobao": "npm config set registry http://registry.npm.taobao.org/",
