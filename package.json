    "clear_taobao": "npm config set registry https://registry.npmjs.org",
    "publish": "npm publish",
    "global_init": "npm install -g npm-check-updates && npm install  -g  rimraf"
  },
  "keywords": [
    "gk-notice",
    "toaster",
    "toast",
