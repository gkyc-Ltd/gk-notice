  "bugs": {
    "url": "https://github.com/dolce/iziToast/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "homepage": "https://github.com/dolce/iziToast#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
